@model IEnumerable<App.GitHub.ViewModels.ProjetoViewModel>

<table class="table  table-hover">
    <thead class="table-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HtmlUrl)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    <a href="@Html.DisplayFor(modelItem => item.HtmlUrl)" target="_blank">@item.HtmlUrl</a>
                </td>
            </tr>
        }
    </tbody>
</table>